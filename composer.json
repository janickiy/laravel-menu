{
    "name": "janickiy/laravel-menu",
    "description": "drag and drop menu generator like wordpress for laravel",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
    	{
            "name": "Rendy Harimayco",
            "email": "rendyharimayco@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Cristian Garcia",
            "email": "informacion@cristiangarcia.co",
            "role" : "Original Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "8.* || 9.* || 10.* || 11.* || 12.*"
    },
    "autoload": {
        "psr-4": {
          "Harimayco\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harimayco\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Harimayco\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
